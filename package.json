{
  "name": "Bolt.js",
  "version": "0.3.0",
  "description": "A Node.js-base runtime environment written in JavaScript",
  "main": "bolt.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/Chieze-Franklin/Bolt.js"
  },
  "scripts": {
    "build": "gulp build",
    "start": "node bolt",
    "test": "npm run test-integration && npm run test-acceptance",
    "test-acceptance": "npm run test-acceptance-tobi && npm run test-acceptance-soda",
    "test-acceptance-soda": "nodeunit test/acceptance_test_with_soda.js",
    "test-acceptance-tobi": "nodeunit test/acceptance_test_with_tobi.js",
    "test-integration": "mocha -R spec test/spec_with_mocha_supertest.js",
    "test-style": "gulp test:style"
  },
  "engines": {
    "node": "6.9.4",
    "npm": "4.1.1"
  },
  "keywords": [
    "bolt",
    "host",
    "jaysos",
    "jsos",
    "node",
    "node.js",
    "nodeos",
    "operating",
    "os",
    "os.js",
    "system",
    "runtime",
    "environment"
  ],
  "dependencies": {
    "body-parser": "^1.15.2",
    "bolt-internal-checks": "*",
    "bolt-internal-config": "*",
    "bolt-internal-defs": "*",
    "bolt-internal-errors": "*",
    "bolt-internal-get-routes": "*",
    "bolt-internal-models": "*",
    "bolt-internal-schemata": "*",
    "bolt-internal-setup": "*",
    "bolt-internal-sockets": "*",
    "bolt-internal-utils": "*",
    "client-sessions": "^0.7.0",
    "cookie-parser": "^1.4.3",
    "express": "~4.14.0",
    "express3-handlebars": "^0.5.2",
    "fs-extra": "^1.0.0",
    "get-package-readme": "^1.2.0",
    "handlebars": "^4.0.5",
    "hbs": "~4.0.0",
    "jwt-simple": "^0.5.1",
    "lodash": "^4.11.1",
    "mongodb": "^2.2.11",
    "mongoose": "^4.6.5",
    "multer": "^1.2.1",
    "mustache": "^2.2.1",
    "npm-programmatic": "0.0.8",
    "pkg.json": "^2.0.5",
    "request": "^2.81.0",
    "showdown": "*",
    "socket.io": "^1.5.1",
    "superagent": "^2.3.0",
    "underscore": "^1.8.3"
  },
  "devDependencies": {
    "coveralls": "^2.12.0",
    "del": "^2.2.2",
    "gulp": "^3.9.1",
    "gulp-jslint": "^1.0.10",
    "jscoverage": "^0.6.0",
    "mocha": "^3.2.0",
    "mocha-lcov-reporter": "^1.3.0",
    "mock-req": "^0.2.0",
    "mock-res": "^0.3.3",
    "nodeunit": "^0.11.0",
    "run-sequence": "^1.2.2",
    "sinon": "^1.17.7",
    "soda": "^0.2.5",
    "supertest": "^3.0.0",
    "tobi": "^0.3.2"
  },
  "optionalDependencies": {},
  "author": "Chieze Franklin",
  "bugs": {
    "url": "https://github.com/Chieze-Franklin/Bolt.js/issues"
  },
  "homepage": "https://chieze-franklin.github.io/Bolt.js/",
  "license": "MIT"
}
